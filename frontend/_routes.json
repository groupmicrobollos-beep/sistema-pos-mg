{
    "version": 1,
    "include": ["/*"],
    "exclude": [],
    "routes": [
        {
            "pattern": "/api/auth/*",
            "methods": ["GET", "POST", "OPTIONS"]
        },
        {
            "pattern": "/api/products",
            "methods": ["GET", "POST", "OPTIONS"]
        },
        {
            "pattern": "/api/products/*",
            "methods": ["GET", "PUT", "DELETE", "OPTIONS"]
        },
        {
            "pattern": "/api/quotes",
            "methods": ["GET", "POST", "OPTIONS"]
        },
        {
            "pattern": "/api/quotes/*",
            "methods": ["GET", "PUT", "DELETE", "OPTIONS"]
        },
        {
            "pattern": "/api/suppliers",
            "methods": ["GET", "POST", "OPTIONS"]
        },
        {
            "pattern": "/api/suppliers/*",
            "methods": ["GET", "PUT", "DELETE", "OPTIONS"]
        },
        {
            "pattern": "/api/system/*",
            "methods": ["GET", "OPTIONS"]
        }
    ]
}